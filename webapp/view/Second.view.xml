<mvc:View
    controllerName="routing.zrouting.controller.Second"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="Second Page"
        titleAlignment="Center"
        showNavButton="true"
        navButtonPress="onBackNavSecond"
        
    >

    <headerContent>
     <Button
         id="idMainButton"
         icon="sap-icon://action"
         type="Transparent"
         press="onPress"
         text="Go to third page"
     /></headerContent>
        <content>
         <Label text="Second View" width="100%"></Label>
         <core:Fragment fragmentName="routing.zrouting.fragment.Demo" type="XML"></core:Fragment>
        </content>
    </Page>
</mvc:View>
